Helpers/helper.php

<?php

//Aktif menü
if (! function_exists('activeMenu')) {
    function activeMenu($url)
    {
        return request()->is($url) ? 'active' : null;
    }
}

//IP adresi alma
if (! function_exists('getIPAddress')) {
    function getIPAddress()
    {
        if (array_key_exists('HTTP_CLIENT_IP', $_SERVER)) {
            $ipAddress = $_SERVER['HTTP_CLIENT_IP'];
        } elseif (array_key_exists('HTTP_X_FORWARDED_FOR', $_SERVER)) {
            $ipAddress = $_SERVER['HTTP_X_FORWARDED_FOR'];
        }

        return $ipAddress ?? null;
    }
}

//Telefon formatlama
if (! function_exists('formatTelephone')) {
    function formatTelephone($telephone) {
        $telephone  = preg_replace("/[^0-9]/", "", $telephone);
        $first = substr("$telephone", 0, 1);
        if ($first == "0") {
            $telephone = substr($telephone, 1);
        }
    
        $tr = substr("$telephone",0,2);

        if ($tr != "90") {
            $formattedTelephone = "Gecersiz: Ülke kodu TR degil.";
        } else {
            $no = substr($telephone, 2);

            if(substr("$no", 0, 1) == "0") {
                $no = substr($no, 1);
            }
    
            if(strlen($no) != "10") {
                $formattedTelephone = "Gecersiz: TR telefon formatina uygun degil (901112223344)"; }
            else {
                $formattedTelephone = "+$tr$no";
            }
        }
    
        return $formattedTelephone;
    }
}
