**************************************************
Services/PrivateKeyService.php

<?php

namespace App\Services;

use Illuminate\Support\Str;

class PrivateKeyService
{
    // Check private key
    public static function checkPrivateKey($model, string $private_key): bool
    {
        $private_keys = $model::pluck('private_key');

        if (collect($private_keys)->contains($private_key)) {
            $privateKeyStatus = True;
        } else {
            $privateKeyStatus = False;
        }

        return $privateKeyStatus;
    }

    // Generate private key
    public static function generatePrivateKey($model): string
    {
        $private_key = Str::random(config('constants.PRIVATE_KEY'));
        $checkPrivateKey = PrivateKeyService::checkPrivateKey($model, $private_key);

        while ($checkPrivateKey == True)
        {
            $private_key = Str::random(config('constants.PRIVATE_KEY'));
            $checkPrivateKey = PrivateKeyService::checkPrivateKey($model, $private_key);
        }

        return $private_key;
    }
}
